{
    "name": "example",
    "version": "v1",
    "dag": [
        {
            "name": "dummy-in",
            "spec": {
                "image": "jz-action/dummy_in:latest",
                "command": "/dummy_in",
                "args": [
                    "--log-level=debug"
                ]
            }
        },
        {
            "name": "copy-in-place",
            "node_type": "ComputeUnit",
            "dependency": [
                "dummy-in"
            ],
            "spec": {
                "image": "jz-action/copy_in_place:latest",
                "command": "/copy_in_place",
                "replicas": 3,
                "args": [
                    "--log-level=debug"
                ]
            },
            "channel": {
                "cache_type": "Memory"
            }
        },
        {
            "name": "dummy-out",
            "node_type": "ComputeUnit",
            "dependency": [
                "copy-in-place"
            ],
            "spec": {
                "image": "jz-action/dummy_out:latest",
                "command": "/dummy_out",
                "replicas": 3,
                "args": [
                    "--log-level=debug"
                ]
            },
            "channel": {
                "cache_type": "Memory"
            }
        }
    ]
}